{
    "name": "viral-vector/laravel-local-events",
    "description": "Search for nearby events via ... API",
    "homepage": "https://github.com/viral-vector/laravel-local-events",
    "license": "MIT",
    "keywords": ["laravel", "events", "local"],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/serializer": "^4.2",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ViralVector\\LocalEvents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViralVector\\LocalEvents\\LocalEventsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
