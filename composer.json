{
    "name": "viral-vector/laravel-local-events",
    "description": "Search for nearby events via ... API",
    "homepage": "https://github.com/viral-vector/laravel-local-events",
    "license": "MIT",
    "keywords": ["laravel", "events", "local"],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ViralVector\\LaravelLocalEvents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViralVector\\LaravelLocalEvents\\LaravelLocalEventsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
